<div class="container mt-5 mb-5">
  <h2>Articles</h2>
</div>

<div class="container mt-5 mb-5">
  <% @articles.each do |article| %>
    <h4><%= article.title %></h4>
    <p><%= article.text %></p>
    <%= link_to 'Show', article_path(article) %> |
    <%= link_to 'Edit', edit_article_path(article) %>
    <hr>
  <% end %>
</div>
